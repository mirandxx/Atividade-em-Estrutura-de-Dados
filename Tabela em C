#include <stdio.h>

int main()
{
    char nomes[5][50]; 
    int datas[5];       
    
  
    printf("Digite o nome de 5 alunos:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%s", nomes[i]);  
    }

    printf("Digite 5 datas de nascimento:\n");
    for (int i = 0; i < 5; i++) {
        scanf("%d", &datas[i]);
    }

    printf("----Tabela----\n");
    for (int i = 0; i < 5; i++) {
        printf("%s - %d\n", nomes[i], datas[i]);
    }

    return 0;
}
